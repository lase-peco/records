{
    "name": "lase-peco/records",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Dabak",
            "email": "a.dabak@lase-peco.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LasePeCo\\Records\\": "src/"
        },

        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LasePeCo\\Records\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.20",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LasePeCo\\Records\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --no-coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
